{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "env": {},
  "regions": ["sfo1"],
  "functions": {
    "src/app/api/**": {
      "maxDuration": 60
    }
  },
  "headers": [
    {
      "source": "/api/posts/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, s-maxage=10, stale-while-revalidate=59"
        }
      ]
    }
  ]
}
