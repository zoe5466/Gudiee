{
  "name": "guidee",
  "version": "0.1.0",
  "private": true,
  "description": "Guidee - 地陪媒合平台，旅遊界的 Uber",
  "keywords": [
    "guidee",
    "travel",
    "tour-guide",
    "platform",
    "next.js"
  ],
  "homepage": "https://guidee.online",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/guidee.git"
  },
  "scripts": {
    "dev": "next dev",
    "build": "cross-env NEXT_TELEMETRY_DISABLED=1 DISABLE_FILE_TRACING=true prisma generate && cross-env NEXT_TELEMETRY_DISABLED=1 DISABLE_FILE_TRACING=true next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:generate": "npx prisma generate",
    "db:migrate": "npx prisma migrate dev",
    "db:reset": "npx prisma migrate reset",
    "db:seed": "npx tsx prisma/seed.ts",
    "db:studio": "npx prisma studio",
    "setup": "npm install && npx prisma generate",
    "postinstall": "prisma generate",
    "fix-modules": "rm -rf node_modules package-lock.json && npm cache clean --force && npm install",
    "fresh-start": "npm run fix-modules && npm run db:generate",
    "deploy": "node scripts/vercel-monitor.js smart-deploy",
    "deploy:auto": "node scripts/auto-deploy-loop.js",
    "deploy:loop": "node scripts/auto-deploy-loop.js",
    "deploy:diagnose": "node scripts/deployment-doctor.js diagnose",
    "deploy:monitor": "node scripts/vercel-monitor.js monitor",
    "deploy:doctor": "node scripts/deployment-doctor.js"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@mapbox/mapbox-gl-geocoder": "^5.0.1",
    "@prisma/client": "^6.14.0",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@stripe/react-stripe-js": "^2.4.0",
    "@stripe/stripe-js": "^2.2.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.16",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "i18next": "^25.3.6",
    "jose": "^5.1.3",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.294.0",
    "mapbox-gl": "^3.0.1",
    "multer": "^1.4.5-lts.1",
    "next": "14.0.4",
    "next-i18next": "^15.4.2",
    "next-themes": "^0.4.6",
    "postcss": "^8.4.32",
    "prisma": "^6.14.0",
    "react": "^18",
    "react-datepicker": "^4.25.0",
    "react-dom": "^18",
    "react-hook-form": "^7.48.2",
    "react-i18next": "^13.5.0",
    "sharp": "^0.33.1",
    "swr": "^2.2.4",
    "tailwind-merge": "^2.1.0",
    "tailwindcss": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5",
    "zod": "^3.22.4",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^13.4.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mapbox-gl": "^3.1.0",
    "@types/multer": "^1.4.11",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "critters": "^0.0.20",
    "cross-env": "^10.0.0",
    "dotenv": "^16.3.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "tsx": "^4.6.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
